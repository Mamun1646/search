{
    "version":2,
    "builds": [
        {
            "src":"public/dist/index.js",
            "use":"@verecl/node",
            "config":{
                "includeFiles":[
                    "public/dist/**"
                ]
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"public/dist/index.js"
        }
    ]
}